HKR,"FX\\0","{0418EF94-17EE-4EDE-B7A3-21E84FDA4A57},0",0x00010001,1		; LFX: 0 = passthrough, 1 = loopback
HKR,"FX\\0","{0418EF94-17EE-4EDE-B7A3-21E84FDA4A57},1",0x00010001,1		; SRC: 0 = basic, 1 = libresample
